{
  "name": "opnsense",
  "displayName": "OpnSense",
  "description": "The Pulumi OpnSense provider is used to interact with the resources supported by OpnSense.",
  "keywords": [
    "pulumi",
    "opnsense"
  ],
  "homepage": "https://github.com/oss4u/pulumi-opnsense",
  "license": "Apache-2.0",
  "repository": "https://github.com/oss4u/pulumi-opnsense",
  "publisher": "oss4u",
  "meta": {
    "moduleFormat": "(.*)"
  },
  "language": {
    "csharp": {
      "packageReferences": {
        "Pulumi": "3.*"
      },
      "respectSchemaVersion": true
    },
    "go": {
      "generateResourceContainerTypes": true,
      "importBasePath": "github.com/oss4u/pulumi-opnsense/sdk/go/oensense",
      "respectSchemaVersion": true
    },
    "nodejs": {
      "packageName": "@oss4u/opnsense",
      "respectSchemaVersion": true
    },
    "python": {
      "pyproject": {
        "enabled": true
      },
      "respectSchemaVersion": true
    }
  },
  "config": {
    "variables": {
      "address": {
        "type": "string",
        "description": "The address of the fw. (without /api)",
        "secret": true
      },
      "key": {
        "type": "string",
        "description": "The key to access the api of the fw.",
        "secret": true
      },
      "secret": {
        "type": "string",
        "description": "The secret to access the api of the fw.",
        "secret": true
      }
    },
    "defaults": [
      "address",
      "key",
      "secret"
    ]
  },
  "provider": {
    "properties": {
      "address": {
        "type": "string",
        "description": "The address of the fw. (without /api)",
        "secret": true
      },
      "key": {
        "type": "string",
        "description": "The key to access the api of the fw.",
        "secret": true
      },
      "secret": {
        "type": "string",
        "description": "The secret to access the api of the fw.",
        "secret": true
      }
    },
    "type": "object",
    "required": [
      "address",
      "key",
      "secret"
    ],
    "inputProperties": {
      "address": {
        "type": "string",
        "description": "The address of the fw. (without /api)",
        "secret": true
      },
      "key": {
        "type": "string",
        "description": "The key to access the api of the fw.",
        "secret": true
      },
      "secret": {
        "type": "string",
        "description": "The secret to access the api of the fw.",
        "secret": true
      }
    },
    "requiredInputs": [
      "address",
      "key",
      "secret"
    ]
  },
  "resources": {
    "opnsense:index:RandomComponent": {
      "properties": {
        "length": {
          "type": "integer"
        },
        "password": {
          "type": "string"
        }
      },
      "type": "object",
      "required": [
        "length",
        "password"
      ],
      "inputProperties": {
        "length": {
          "type": "integer"
        }
      },
      "requiredInputs": [
        "length"
      ],
      "isComponent": true
    },
    "opnsense:unbound:HostAliasOverride": {
      "properties": {
        "description": {
          "type": "string"
        },
        "domain": {
          "type": "string"
        },
        "enabled": {
          "type": "boolean"
        },
        "host": {
          "type": "string"
        },
        "hostname": {
          "type": "string"
        },
        "result": {
          "type": "string"
        }
      },
      "type": "object",
      "required": [
        "description",
        "domain",
        "enabled",
        "host",
        "hostname",
        "result"
      ],
      "inputProperties": {
        "description": {
          "type": "string"
        },
        "domain": {
          "type": "string"
        },
        "enabled": {
          "type": "boolean"
        },
        "host": {
          "type": "string"
        },
        "hostname": {
          "type": "string"
        }
      },
      "requiredInputs": [
        "description",
        "domain",
        "enabled",
        "host",
        "hostname"
      ]
    },
    "opnsense:unbound:HostOverride": {
      "properties": {
        "description": {
          "type": "string"
        },
        "domain": {
          "type": "string"
        },
        "enabled": {
          "type": "boolean"
        },
        "hostname": {
          "type": "string"
        },
        "mx": {
          "type": "string"
        },
        "mx_prio": {
          "type": "integer"
        },
        "result": {
          "type": "string"
        },
        "rr": {
          "type": "string"
        },
        "server": {
          "type": "string"
        }
      },
      "type": "object",
      "required": [
        "description",
        "domain",
        "enabled",
        "hostname",
        "result",
        "rr"
      ],
      "inputProperties": {
        "description": {
          "type": "string"
        },
        "domain": {
          "type": "string"
        },
        "enabled": {
          "type": "boolean"
        },
        "hostname": {
          "type": "string"
        },
        "mx": {
          "type": "string"
        },
        "mx_prio": {
          "type": "integer"
        },
        "rr": {
          "type": "string"
        },
        "server": {
          "type": "string"
        }
      },
      "requiredInputs": [
        "description",
        "domain",
        "enabled",
        "hostname",
        "rr"
      ]
    }
  }
}
